<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#F7770E" />

            <corners android:radius="10dp" />
        </shape></item>
    <item><shape>
            <gradient android:angle="270" android:centerColor="#F69B11" android:endColor="#F7770E" android:startColor="#FCBC15" />

            <corners android:radius="10dp" />
        </shape></item>

</selector>